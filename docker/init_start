#!/bin/bash


docker exec -it  php-fpm_project_name chmod -R 777 .
docker exec -it  php-fpm_project_name bin/magento s:up
docker exec -it  php-fpm_project_name chmod -R 777 .
docker exec -it  php-fpm_project_name bin/magento s:d:c
docker exec -it  php-fpm_project_name chmod -R 777 .
docker exec -it  php-fpm_project_name bin/magento s:s:d -f
docker exec -it  php-fpm_project_name chmod -R 777 .
docker exec -it  php-fpm_project_name bin/magento c:c
docker exec -it  php-fpm_project_name chmod -R 777 .
